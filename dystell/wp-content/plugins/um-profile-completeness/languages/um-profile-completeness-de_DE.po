#: includes/admin/templates/role/profilecompleteness.php:12
msgid "Enable profile completeness"
msgstr "Aktivieren für die Profilvollständigkeit"

#: includes/admin/templates/role/profilecompleteness.php:13
msgid "Turn on / off profile completeness features for this role"
msgstr ""
"Aktivieren / Deaktivieren der Profilvollständigkeitsfunktionen für diese "
"Rolle"

#: includes/admin/templates/role/profilecompleteness.php:16
#: includes/admin/templates/role/profilecompleteness.php:51
#: includes/admin/templates/role/profilecompleteness.php:91
#: includes/admin/templates/role/profilecompleteness.php:103
#: includes/admin/templates/role/profilecompleteness.php:115
msgid "No"
msgstr "Nein"

